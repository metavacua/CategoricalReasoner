{
  "@context": {
    "@version": 1.1,
    "catty": "https://metavacua.github.io/CategoricalReasoner/ontology/",
    "owl": "http://www.w3.org/2002/07/owl#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "dct": "http://purl.org/dc/terms/",
    "prov": "http://www.w3.org/ns/prov#",
    "math": "http://www.w3.org/2000/10/swap/math#"
  },
  "@graph": [
    {
      "@id": "catty:LogicCategoryStructure",
      "@type": "catty:Category",
      "rdfs:label": "Logic Category Structure",
      "rdfs:comment": "The categorical structure of formal logics where LM is the initial base and LK is the terminal system.",
      "dct:description": "Morphisms in this category represent extensions or interpretations. The structure is characterized by commutative diagrams relating minimal logics, intuitionistic variants, and the terminal classical system."
    },
    {
      "@id": "catty:LatticeOrder",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "extends",
      "rdfs:comment": "The extension relation: Logic A extends Logic B if A validates all sequents of B plus additional ones, or removes restrictions.",
      "rdfs:domain": "catty:Logic",
      "rdfs:range": "catty:Logic"
    },
    {
      "@id": "catty:LatticeNode_LM",
      "@type": "catty:LatticeNode",
      "rdfs:label": "Node: LM",
      "catty:logic": "catty:LM",
      "rdfs:comment": "Initial node containing basic conjunction and disjunction."
    },
    {
      "@id": "catty:LatticeNode_LJ",
      "@type": "catty:LatticeNode",
      "rdfs:label": "Node: LJ",
      "catty:logic": "catty:LJ",
      "rdfs:comment": "Intuitionistic branch. Restricts RHS."
    },
    {
      "@id": "catty:LatticeNode_LDJ",
      "@type": "catty:LatticeNode",
      "rdfs:label": "Node: LDJ",
      "catty:logic": "catty:LDJ",
      "rdfs:comment": "Dual intuitionistic branch. Restricts LHS."
    },
    {
      "@id": "catty:LatticeNode_LK",
      "@type": "catty:LatticeNode",
      "rdfs:label": "Node: LK",
      "catty:logic": "catty:LK",
      "rdfs:comment": "Terminal node. Classical logic."
    },
    {
      "@id": "catty:LatticeNode_LL",
      "@type": "catty:LatticeNode",
      "rdfs:label": "Node: LL",
      "catty:logic": "catty:LL",
      "rdfs:comment": "Linear logic core. Resource sensitive."
    },
    {
      "@id": "catty:CommutativeDiagram_Intuitionistic",
      "@type": "prov:Entity",
      "rdfs:label": "Intuitionistic Commutative Diagram",
      "rdfs:comment": "LM -> LJ -> LK and LM -> LDJ -> LK"
    },
    {
      "@id": "catty:CommutativeDiagram_Substructural",
      "@type": "prov:Entity",
      "rdfs:label": "Substructural Commutative Diagram",
      "rdfs:comment": "LL -> ALL -> LK and LL -> RLL -> LK"
    }
  ]
}
