{
  "@context": [
    "context.jsonld",
    {
      "@base": "http://localhost:8080/ontology/catty-categorical-schema#"
    }
  ],
  "@graph": [
    {
      "@id": "Category",
      "@type": "owl:Class",
      "rdfs:label": "Category",
      "rdfs:comment": "A category consisting of objects and morphisms with composition"
    },
    {
      "@id": "Object",
      "@type": "owl:Class",
      "rdfs:label": "Object",
      "rdfs:comment": "An object in a category",
      "rdfs:subClassOf": {
        "@type": "owl:Class",
        "owl:equivalentClass": "catty:CategoryObject"
      }
    },
    {
      "@id": "Morphism",
      "@type": "owl:Class",
      "rdfs:label": "Morphism",
      "rdfs:comment": "A morphism (arrow) in a category",
      "rdfs:subClassOf": {
        "@type": "owl:Restriction",
        "owl:onProperty": "codomain",
        "owl:cardinality": 1
      }
    },
    {
      "@id": "domain",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "domain",
      "rdfs:comment": "The domain (source) of a morphism",
      "rdfs:domain": "Morphism",
      "rdfs:range": "Object"
    },
    {
      "@id": "codomain",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "codomain",
      "rdfs:comment": "The codomain (target) of a morphism",
      "rdfs:domain": "Morphism",
      "rdfs:range": "Object"
    },
    {
      "@id": "composition",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "composition",
      "rdfs:comment": "Composition of morphisms",
      "rdfs:domain": "Morphism",
      "rdfs:range": "Morphism"
    },
    {
      "@id": "Functor",
      "@type": "owl:Class",
      "rdfs:label": "Functor",
      "rdfs:comment": "A functor mapping between categories, preserving structure",
      "rdfs:subClassOf": {
        "@type": "owl:Restriction",
        "owl:onProperty": "targetCategory",
        "owl:cardinality": 1
      }
    },
    {
      "@id": "sourceCategory",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "source category",
      "rdfs:domain": "Functor",
      "rdfs:range": "Category"
    },
    {
      "@id": "targetCategory",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "target category",
      "rdfs:domain": "Functor",
      "rdfs:range": "Category"
    },
    {
      "@id": "NaturalTransformation",
      "@type": "owl:Class",
      "rdfs:label": "Natural Transformation",
      "rdfs:comment": "A natural transformation between functors",
      "rdfs:subClassOf": {
        "@type": "owl:Restriction",
        "owl:onProperty": "targetFunctor",
        "owl:cardinality": 1
      }
    },
    {
      "@id": "sourceFunctor",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "source functor",
      "rdfs:domain": "NaturalTransformation",
      "rdfs:range": "Functor"
    },
    {
      "@id": "targetFunctor",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "target functor",
      "rdfs:domain": "NaturalTransformation",
      "rdfs:range": "Functor"
    },
    {
      "@id": "AdjointFunctors",
      "@type": "owl:Class",
      "rdfs:label": "Adjoint Functors",
      "rdfs:comment": "Pair of functors in adjunction (left and right adjoint)"
    },
    {
      "@id": "EquivalenceOfCategories",
      "@type": "owl:Class",
      "rdfs:label": "Equivalence of Categories",
      "rdfs:comment": "An equivalence relation between categories"
    },
    {
      "@id": "LogicCategory",
      "@type": "owl:Class",
      "rdfs:label": "Logic Category",
      "rdfs:comment": "A category whose objects are formal logics and morphisms are proof-theoretic relationships",
      "rdfs:subClassOf": "Category"
    },
    {
      "@id": "LogicalEntity",
      "@type": "owl:Class",
      "rdfs:label": "Logical Entity",
      "rdfs:comment": "An entity in the logical domain (logic or theory)",
      "rdfs:subClassOf": "Object"
    },
    {
      "@id": "Logic",
      "@type": "owl:Class",
      "rdfs:label": "Logic",
      "rdfs:comment": "A formal logic characterized by a logical signature and logical axioms",
      "rdfs:subClassOf": "LogicalEntity"
    },
    {
      "@id": "LogicalTheory",
      "@type": "owl:Class",
      "rdfs:label": "Logical Theory",
      "rdfs:comment": "A theory consisting of a logical signature, logical axioms, theoretical signature, and theoretical axioms",
      "rdfs:subClassOf": "LogicalEntity"
    },
    {
      "@id": "LogicalSignature",
      "@type": "owl:Class",
      "rdfs:label": "Logical Signature",
      "rdfs:comment": "The set of symbols and connectives used by a logic"
    },
    {
      "@id": "TheoreticalSignature",
      "@type": "owl:Class",
      "rdfs:label": "Theoretical Signature",
      "rdfs:comment": "The set of symbols and constants used by a specific theory"
    },
    {
      "@id": "LogicalAxiom",
      "@type": "owl:Class",
      "rdfs:label": "Logical Axiom",
      "rdfs:comment": "An axiom that is part of the logic's definition"
    },
    {
      "@id": "TheoreticalAxiom",
      "@type": "owl:Class",
      "rdfs:label": "Theoretical Axiom",
      "rdfs:comment": "An axiom specific to a theory, supported by a logic"
    },
    {
      "@id": "hasLogicalSignature",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "has logical signature",
      "rdfs:domain": "LogicalEntity",
      "rdfs:range": "LogicalSignature"
    },
    {
      "@id": "hasTheoreticalSignature",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "has theoretical signature",
      "rdfs:domain": "LogicalTheory",
      "rdfs:range": "TheoreticalSignature"
    },
    {
      "@id": "hasLogicalAxiom",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "has logical axiom",
      "rdfs:domain": "LogicalEntity",
      "rdfs:range": "LogicalAxiom"
    },
    {
      "@id": "hasTheoreticalAxiom",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "has theoretical axiom",
      "rdfs:domain": "LogicalTheory",
      "rdfs:range": "TheoreticalAxiom"
    },
    {
      "@id": "LogicMorphism",
      "@type": "owl:Class",
      "rdfs:label": "Logic Morphism",
      "rdfs:comment": "A morphism between logics or theories (extension, interpretation, or restriction)",
      "rdfs:subClassOf": "Morphism"
    },
    {
      "@id": "Extension",
      "@type": "owl:Class",
      "rdfs:label": "Extension",
      "rdfs:comment": "A morphism where the target logic extends the source logic (validates more sequents or adds rules/axioms)",
      "rdfs:subClassOf": "LogicMorphism"
    },
    {
      "@id": "AddAxiom",
      "@type": "owl:Class",
      "rdfs:label": "Add Axiom",
      "rdfs:comment": "An extension morphism that adds a specific logical axiom",
      "rdfs:subClassOf": "Extension"
    },
    {
      "@id": "AddLEM",
      "@type": "owl:Class",
      "rdfs:label": "Add LEM",
      "rdfs:comment": "Extension by adding the Law of Excluded Middle",
      "rdfs:subClassOf": "AddAxiom"
    },
    {
      "@id": "AddLNC",
      "@type": "owl:Class",
      "rdfs:label": "Add LNC",
      "rdfs:comment": "Extension by adding the Law of Non-Contradiction",
      "rdfs:subClassOf": "AddAxiom"
    },
    {
      "@id": "AddExplosion",
      "@type": "owl:Class",
      "rdfs:label": "Add Explosion",
      "rdfs:comment": "Extension by adding the Principle of Explosion (Ex Falso Quodlibet)",
      "rdfs:subClassOf": "AddAxiom"
    },
    {
      "@id": "AddStructuralRule",
      "@type": "owl:Class",
      "rdfs:label": "Add Structural Rule",
      "rdfs:comment": "An extension morphism that adds a structural rule (LHS, RHS, or both)",
      "rdfs:subClassOf": "Extension"
    },
    {
      "@id": "Interpretation",
      "@type": "owl:Class",
      "rdfs:label": "Interpretation",
      "rdfs:comment": "A morphism representing an interpretability relationship between logics",
      "rdfs:subClassOf": "LogicMorphism"
    },
    {
      "@id": "hasSequentForm",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "has sequent form",
      "rdfs:comment": "The sequent form used by a logic",
      "rdfs:domain": "Logic",
      "rdfs:range": "SequentForm"
    },
    {
      "@id": "SequentForm",
      "@type": "owl:Class",
      "rdfs:label": "Sequent Form",
      "rdfs:comment": "The form of sequents (e.g., single-conclusion, multi-conclusion, hypersequent)"
    },
    {
      "@id": "hasStructuralRule",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "has structural rule",
      "rdfs:comment": "A structural rule available in the logic",
      "rdfs:domain": "Logic",
      "rdfs:range": "StructuralRule"
    },
    {
      "@id": "StructuralRule",
      "@type": "owl:Class",
      "rdfs:label": "Structural Rule",
      "rdfs:comment": "A structural rule (weakening, contraction, exchange) which may be applied to the LHS (antecedent) or RHS (succedent) of a sequent"
    },
    {
      "@id": "Weakening",
      "@type": "owl:Class",
      "rdfs:subClassOf": "StructuralRule",
      "rdfs:label": "Weakening",
      "rdfs:comment": "The weakening structural rule (addition of arbitrary formulas). Its absence implies a 'no erasure' principle."
    },
    {
      "@id": "LHSWeakening",
      "@type": "Weakening",
      "rdfs:label": "LHS Weakening",
      "rdfs:comment": "Weakening on the left hand side (antecedent) of entailment"
    },
    {
      "@id": "RHSWeakening",
      "@type": "Weakening",
      "rdfs:label": "RHS Weakening",
      "rdfs:comment": "Weakening on the right hand side (succedent) of entailment"
    },
    {
      "@id": "Contraction",
      "@type": "owl:Class",
      "rdfs:subClassOf": "StructuralRule",
      "rdfs:label": "Contraction",
      "rdfs:comment": "The contraction structural rule (duplication of formulas). Its absence implies a 'no cloning' principle."
    },
    {
      "@id": "LHSContraction",
      "@type": "Contraction",
      "rdfs:label": "LHS Contraction",
      "rdfs:comment": "Contraction on the left hand side (antecedent) of entailment"
    },
    {
      "@id": "RHSContraction",
      "@type": "Contraction",
      "rdfs:label": "RHS Contraction",
      "rdfs:comment": "Contraction on the right hand side (succedent) of entailment"
    },
    {
      "@id": "Exchange",
      "@type": "owl:Class",
      "rdfs:subClassOf": "StructuralRule",
      "rdfs:label": "Exchange",
      "rdfs:comment": "The exchange structural rule (reordering of formulas). Its absence implies a 'no partial swapping' principle, as in non-commutative or order logics."
    },
    {
      "@id": "LHSExchange",
      "@type": "Exchange",
      "rdfs:label": "LHS Exchange",
      "rdfs:comment": "Exchange on the left hand side (antecedent) of entailment"
    },
    {
      "@id": "RHSExchange",
      "@type": "Exchange",
      "rdfs:label": "RHS Exchange",
      "rdfs:comment": "Exchange on the right hand side (succedent) of entailment"
    },
    {
      "@id": "latticeCoordinate",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "lattice coordinate",
      "rdfs:comment": "Position in 2D lattice as coordinate pair (x,y)",
      "rdfs:domain": "Logic",
      "rdfs:range": "xsd:string"
    },
    {
      "@id": "SequentRestrictionMorphism",
      "@type": "owl:Class",
      "rdfs:label": "Sequent Restriction Morphism",
      "rdfs:comment": "A morphism that imposes sequent form restrictions",
      "rdfs:subClassOf": "LogicMorphism"
    },
    {
      "@id": "StructuralRuleMorphism",
      "@type": "owl:Class",
      "rdfs:label": "Structural Rule Morphism",
      "rdfs:comment": "A morphism that adds or removes structural rules",
      "rdfs:subClassOf": "LogicMorphism"
    },
    {
      "@id": "ProofSystemEmbedding",
      "@type": "owl:Class",
      "rdfs:label": "Proof System Embedding",
      "rdfs:comment": "A functorial embedding of one proof system into another",
      "rdfs:subClassOf": {
        "@type": "owl:Class",
        "owl:intersectionOf": [
          "LogicMorphism",
          "Functor"
        ]
      }
    },
    {
      "@id": "LogicLattice",
      "@type": "owl:Class",
      "rdfs:label": "Logic Lattice",
      "rdfs:comment": "The 2D poset category of logics organized by sequent restrictions (horizontal) and structural rules (vertical)",
      "rdfs:subClassOf": "PosetCategory"
    },
    {
      "@id": "PosetCategory",
      "@type": "owl:Class",
      "rdfs:label": "Poset Category",
      "rdfs:comment": "A category representing a partially ordered set (at most one morphism between any two objects)",
      "rdfs:subClassOf": "Category"
    },
    {
      "@id": "CurryHowardEquivalence",
      "@type": "owl:Class",
      "rdfs:label": "Curry-Howard Equivalence",
      "rdfs:comment": "The categorical equivalence between Logic-as-Category and Type-Theory-as-Category",
      "rdfs:subClassOf": "EquivalenceOfCategories"
    },
    {
      "@id": "LogicAsCategory",
      "@type": "owl:Class",
      "rdfs:label": "Logic as Category",
      "rdfs:comment": "Category of logics with proof-theoretic morphisms",
      "rdfs:subClassOf": "LogicCategory"
    },
    {
      "@id": "TypeTheoryAsCategory",
      "@type": "owl:Class",
      "rdfs:label": "Type Theory as Category",
      "rdfs:comment": "Category of type theories with type-theoretic morphisms",
      "rdfs:subClassOf": "Category"
    },
    {
      "@id": "Type",
      "@type": "owl:Class",
      "rdfs:label": "Type",
      "rdfs:comment": "A type in type theory, corresponding to a proposition",
      "rdfs:subClassOf": "Object"
    },
    {
      "@id": "TypeMorphism",
      "@type": "owl:Class",
      "rdfs:label": "Type Morphism",
      "rdfs:comment": "A morphism between types (functions/programs)",
      "rdfs:subClassOf": "Morphism"
    },
    {
      "@id": "proofAsProgram",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "proof as program",
      "rdfs:comment": "Correspondence between a proof transformation and a program transformation under Curry-Howard",
      "rdfs:domain": "LogicMorphism",
      "rdfs:range": "TypeMorphism"
    },
    {
      "@id": "propositionAsType",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "proposition as type",
      "rdfs:comment": "Correspondence between a proposition and a type under Curry-Howard",
      "rdfs:domain": "Logic",
      "rdfs:range": "Type"
    }
  ]
}
