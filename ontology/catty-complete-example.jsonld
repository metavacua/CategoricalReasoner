{
  "@context": [
    "context.jsonld",
    {
      "@base": "http://localhost:8080/ontology/catty-complete-example#"
    }
  ],
  "@graph": [
    {
      "@id": "CompleteLatticeExample",
      "@type": "catty:LogicLattice",
      "rdfs:label": "Complete Example: Logic Category",
      "rdfs:comment": "A category where objects are logics and morphisms are extension or interpretation relationships.",
      "dct:description": "This example demonstrates how logics are related via the addition of axioms (LEM, LNC, Explosion) and structural rules. LK is the terminal logic validating all classical principles. Substructural logics are characterized by the absence of specific structural rules, which corresponds to the absence of certain logical axioms."
    },
    {
      "@id": "LM",
      "@type": "catty:Logic",
      "rdfs:label": "LM (Minimal Base)",
      "rdfs:comment": "Initial logic with conjunction and disjunction. No negation.",
      "catty:hasLogicalSignature": "lao:StandardSignature"
    },
    {
      "@id": "LJ",
      "@type": "catty:Logic",
      "rdfs:label": "LJ (Intuitionistic)",
      "rdfs:comment": "Extends LM, validates LNC and Explosion. Single-conclusion sequents.",
      "catty:hasLogicalSignature": "lao:StandardSignature",
      "catty:hasLogicalAxiom": [
        "lao:Axiom_LNC",
        "lao:Axiom_Explosion"
      ]
    },
    {
      "@id": "LDJ",
      "@type": "catty:Logic",
      "rdfs:label": "LDJ (Dual Intuitionistic)",
      "rdfs:comment": "Extends LM, validates LEM. Single-antecedent sequents.",
      "catty:hasLogicalSignature": "lao:StandardSignature",
      "catty:hasLogicalAxiom": "lao:Axiom_LEM"
    },
    {
      "@id": "LK",
      "@type": "catty:Logic",
      "rdfs:label": "LK (Classical)",
      "rdfs:comment": "Terminal logic. Validates LEM, LNC, Explosion, and DNE. Full structural rules.",
      "catty:hasLogicalSignature": "lao:StandardSignature",
      "catty:hasLogicalAxiom": [
        "lao:Axiom_LEM",
        "lao:Axiom_LNC",
        "lao:Axiom_Explosion",
        "lao:Axiom_DNE"
      ]
    },
    {
      "@id": "LL",
      "@type": "catty:Logic",
      "rdfs:label": "LL (Linear)",
      "rdfs:comment": "No cloning (contraction) and No erasure (weakening).",
      "catty:hasLogicalSignature": "lao:StandardSignature"
    },
    {
      "@id": "ALL",
      "@type": "catty:Logic",
      "rdfs:label": "ALL (Affine)",
      "rdfs:comment": "Linear logic + Weakening. Allows erasure.",
      "catty:hasLogicalSignature": "lao:StandardSignature"
    },
    {
      "@id": "RLL",
      "@type": "catty:Logic",
      "rdfs:label": "RLL (Relevant)",
      "rdfs:comment": "Linear logic + Contraction. Allows cloning.",
      "catty:hasLogicalSignature": "lao:StandardSignature"
    },
    {
      "@id": "Morphism_LM_to_LJ",
      "@type": "catty:Extension",
      "catty:domain": "LM",
      "catty:codomain": "LJ",
      "rdfs:comment": "Adds LNC and Explosion."
    },
    {
      "@id": "Morphism_LM_to_LDJ",
      "@type": "catty:Extension",
      "catty:domain": "LM",
      "catty:codomain": "LDJ",
      "rdfs:comment": "Adds LEM."
    },
    {
      "@id": "Morphism_LJ_to_LK",
      "@type": "catty:AddLEM",
      "catty:domain": "LJ",
      "catty:codomain": "LK"
    },
    {
      "@id": "Morphism_LDJ_to_LK",
      "@type": "catty:AddLNC",
      "catty:domain": "LDJ",
      "catty:codomain": "LK"
    },
    {
      "@id": "Morphism_LL_to_ALL",
      "@type": "catty:AddStructuralRule",
      "catty:domain": "LL",
      "catty:codomain": "ALL",
      "rdfs:label": "Add Weakening"
    },
    {
      "@id": "Morphism_LL_to_RLL",
      "@type": "catty:AddStructuralRule",
      "catty:domain": "LL",
      "catty:codomain": "RLL",
      "rdfs:label": "Add Contraction"
    },
    {
      "@id": "lao:Axiom_LEM",
      "@type": "catty:LogicalAxiom",
      "rdfs:label": "LEM",
      "rdfs:comment": "A ∨ ¬A"
    },
    {
      "@id": "lao:Axiom_LNC",
      "@type": "catty:LogicalAxiom",
      "rdfs:label": "LNC",
      "rdfs:comment": "¬(A ∧ ¬A)"
    },
    {
      "@id": "lao:Axiom_Explosion",
      "@type": "catty:LogicalAxiom",
      "rdfs:label": "Explosion",
      "rdfs:comment": "⊥ ⊢ A"
    },
    {
      "@id": "lao:Axiom_DNE",
      "@type": "catty:LogicalAxiom",
      "rdfs:label": "DNE",
      "rdfs:comment": "¬¬A ⊢ A"
    }
  ]
}
