{
  "@context": {
    "@version": 1.1,
    "catty": "http://catty.org/ontology/",
    "owl": "http://www.w3.org/2002/07/owl#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "dct": "http://purl.org/dc/terms/",
    "prov": "http://www.w3.org/ns/prov#",
    "wd": "http://www.wikidata.org/entity/",
    "dbo": "http://dbpedia.org/ontology/",
    "skos": "http://www.w3.org/2004/02/skos/core#"
  },
  "@graph": [
    {
      "@id": "catty:CompleteLatticeExample",
      "@type": "catty:LogicLattice",
      "rdfs:label": "Complete Example: Logic Lattice Category",
      "rdfs:comment": "A category where objects are logics and morphisms are extension or interpretation relationships. The lattice structure emerges from the restriction and extension of logical rules and axioms.",
      "dct:description": "The logic category formalizes the space of formal systems. LK (Classical Logic) acts as the terminal logic, validating both the Law of Excluded Middle (LEM) and the Law of Non-Contradiction (LNC). LM acts as a common minimal sublogic for LJ and LDJ. Substructural logics (LL, ALL, RLL) are characterized by the absence of specific structural rules on the LHS and RHS."
    },
    {
      "@id": "catty:LM",
      "@type": "catty:Logic",
      "rdfs:label": "LM (Minimal Base)",
      "rdfs:comment": "Initial logic with conjunction and disjunction, but no negation.",
      "catty:hasLogicalSignature": "catty:LM_Signature"
    },
    {
      "@id": "catty:LJ",
      "@type": "catty:Logic",
      "rdfs:label": "LJ (Intuitionistic)",
      "rdfs:comment": "Extends LM, validates LNC, invalidates one direction of DNE. Restricts RHS to single formula.",
      "catty:hasSequentForm": "catty:SingleConclusionSequent",
      "catty:hasStructuralRule": [
        "catty:LHSWeakening", "catty:RHSWeakening",
        "catty:LHSContraction", "catty:RHSContraction",
        "catty:LHSExchange", "catty:RHSExchange"
      ],
      "catty:hasLogicalAxiom": "catty:Axiom_LNC"
    },
    {
      "@id": "catty:LDJ",
      "@type": "catty:Logic",
      "rdfs:label": "LDJ (Dual Intuitionistic)",
      "rdfs:comment": "Extends LM, validates LEM, invalidates the other direction of DNE. Restricts LHS to single formula.",
      "catty:hasSequentForm": "catty:SingleAntecedentSequent",
      "catty:hasStructuralRule": [
        "catty:LHSWeakening", "catty:RHSWeakening",
        "catty:LHSContraction", "catty:RHSContraction",
        "catty:LHSExchange", "catty:RHSExchange"
      ],
      "catty:hasLogicalAxiom": "catty:Axiom_LEM"
    },
    {
      "@id": "catty:LK",
      "@type": "catty:Logic",
      "rdfs:label": "LK (Classical)",
      "rdfs:comment": "Terminal logic. Validates LEM and LNC. Full multi-conclusion sequents and structural rules.",
      "catty:hasSequentForm": "catty:MultiConclusionSequent",
      "catty:hasStructuralRule": [
        "catty:LHSWeakening", "catty:RHSWeakening",
        "catty:LHSContraction", "catty:RHSContraction",
        "catty:LHSExchange", "catty:RHSExchange"
      ],
      "catty:hasLogicalAxiom": ["catty:Axiom_LEM", "catty:Axiom_LNC"]
    },
    {
      "@id": "catty:LL",
      "@type": "catty:Logic",
      "rdfs:label": "LL (Linear)",
      "rdfs:comment": "Substructural logic without weakening or contraction. No Cloning (contraction absence) and No Erasure (weakening absence).",
      "catty:hasStructuralRule": ["catty:LHSExchange", "catty:RHSExchange"]
    },
    {
      "@id": "catty:ALL",
      "@type": "catty:Logic",
      "rdfs:label": "ALL (Affine)",
      "rdfs:comment": "Linear logic + Weakening. Allows erasure but no cloning.",
      "catty:hasStructuralRule": [
        "catty:LHSWeakening", "catty:RHSWeakening",
        "catty:LHSExchange", "catty:RHSExchange"
      ]
    },
    {
      "@id": "catty:RLL",
      "@type": "catty:Logic",
      "rdfs:label": "RLL (Relevant)",
      "rdfs:comment": "Linear logic + Contraction. Allows cloning but no erasure.",
      "catty:hasStructuralRule": [
        "catty:LHSContraction", "catty:RHSContraction",
        "catty:LHSExchange", "catty:RHSExchange"
      ]
    },
    {
      "@id": "catty:Morphism_LM_to_LJ",
      "@type": "catty:Extension",
      "catty:domain": "catty:LM",
      "catty:codomain": "catty:LJ"
    },
    {
      "@id": "catty:Morphism_LM_to_LDJ",
      "@type": "catty:Extension",
      "catty:domain": "catty:LM",
      "catty:codomain": "catty:LDJ"
    },
    {
      "@id": "catty:Morphism_LJ_to_LK",
      "@type": "catty:Extension",
      "catty:domain": "catty:LJ",
      "catty:codomain": "catty:LK"
    },
    {
      "@id": "catty:Morphism_LDJ_to_LK",
      "@type": "catty:Extension",
      "catty:domain": "catty:LDJ",
      "catty:codomain": "catty:LK"
    },
    {
      "@id": "catty:Morphism_LL_to_ALL",
      "@type": "catty:Extension",
      "catty:domain": "catty:LL",
      "catty:codomain": "catty:ALL"
    },
    {
      "@id": "catty:Morphism_LL_to_RLL",
      "@type": "catty:Extension",
      "catty:domain": "catty:LL",
      "catty:codomain": "catty:RLL"
    },
    {
      "@id": "catty:Morphism_ALL_to_LK",
      "@type": "catty:Extension",
      "catty:domain": "catty:ALL",
      "catty:codomain": "catty:LK"
    },
    {
      "@id": "catty:Morphism_RLL_to_LK",
      "@type": "catty:Extension",
      "catty:domain": "catty:RLL",
      "catty:codomain": "catty:LK"
    },
    {
      "@id": "catty:Axiom_LEM",
      "@type": "catty:LogicalAxiom",
      "rdfs:label": "LEM",
      "rdfs:comment": "A ∨ ¬A"
    },
    {
      "@id": "catty:Axiom_LNC",
      "@type": "catty:LogicalAxiom",
      "rdfs:label": "LNC",
      "rdfs:comment": "¬(A ∧ ¬A)"
    }
  ]
}
