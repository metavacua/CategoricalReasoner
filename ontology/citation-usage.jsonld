{
  "@context": {
    "@vocab": "http://metavacua.github.io/catty/ontology#",
    "catty": "http://metavacua.github.io/catty/",
    "dct": "http://purl.org/dc/terms/",
    "prov": "http://www.w3.org/ns/prov#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "iso690": "http://purl.org/iso690/",
    "references": {
      "@id": "dct:references",
      "@type": "@id"
    },
    "isReferencedBy": {
      "@id": "dct:isReferencedBy",
      "@type": "@id"
    },
    "wasDerivedFrom": {
      "@id": "prov:wasDerivedFrom",
      "@type": "@id"
    },
    "hadPrimarySource": {
      "@id": "prov:hadPrimarySource",
      "@type": "@id"
    }
  },
  "@graph": [
    {
      "@id": "catty:citation-usage",
      "@type": "prov:Collection",
      "dct:title": "Catty Thesis Citation Usage Model",
      "dct:description": "Tracks which theorems, definitions, and sections cite which sources (SWTI S2 compliance, ISO 690 conformance)",
      "dct:created": "2026-01-05",
      "rdfs:comment": "This graph will be populated dynamically as thesis content is created. Each TeX element that cites a source must have a corresponding RDF provenance link."
    },
    {
      "@id": "catty:provenance-example-1",
      "@type": "prov:Attribution",
      "rdfs:comment": "Example: A theorem on weakening cites Girard's linear logic paper",
      "prov:entity": {"@id": "catty:thm-weakening"},
      "prov:hadPrimarySource": {"@id": "catty:citations/girard1987linear"},
      "dct:references": {"@id": "catty:citations/girard1987linear"}
    },
    {
      "@id": "catty:provenance-example-2",
      "@type": "prov:Derivation",
      "rdfs:comment": "Example: A section on linear logic is derived from Girard's work",
      "prov:entity": {"@id": "catty:sec-linear-logic"},
      "prov:wasDerivedFrom": {"@id": "catty:citations/girard1987linear"}
    },
    {
      "@id": "catty:provenance-example-3",
      "@type": "prov:Attribution",
      "rdfs:comment": "Example: Definition of intuitionistic logic references Kripke semantics",
      "prov:entity": {"@id": "catty:def-intuitionistic-logic"},
      "prov:hadPrimarySource": {"@id": "catty:citations/kripke1965semantical"},
      "dct:references": {"@id": "catty:citations/kripke1965semantical"}
    },
    {
      "@id": "catty:provenance-example-4",
      "@type": "prov:Attribution",
      "rdfs:comment": "Example: Category theory section cites Mac Lane",
      "prov:entity": {"@id": "catty:sec-category-theory-foundations"},
      "dct:references": [
        {"@id": "catty:citations/maclane1971categories"},
        {"@id": "catty:citations/lawvere1963functorial"}
      ]
    },
    {
      "@id": "catty:provenance-example-5",
      "@type": "prov:Attribution",
      "rdfs:comment": "Example: Curry-Howard correspondence section cites both foundational papers",
      "prov:entity": {"@id": "catty:sec-curry-howard"},
      "dct:references": [
        {"@id": "catty:citations/curry1958combinatory"},
        {"@id": "catty:citations/howard1980formulae"}
      ],
      "prov:wasDerivedFrom": [
        {"@id": "catty:citations/curry1958combinatory"},
        {"@id": "catty:citations/howard1980formulae"}
      ]
    }
  ],
  "iso690:conformance": {
    "@type": "iso690:BibliographicStandard",
    "rdfs:comment": "All citations conform to ISO 690:2021 standard for bibliographic references",
    "iso690:style": "author-date",
    "iso690:elements": [
      "creator (author/editor)",
      "title",
      "publication year",
      "publisher (for books)",
      "journal name and volume (for articles)",
      "DOI or persistent identifier",
      "external resource links (Wikidata, DBpedia)"
    ]
  },
  "validation-rules": {
    "@type": "catty:ValidationRules",
    "rdfs:comment": "Rules enforced by citation validator",
    "rules": [
      "Every \\cite{key} in TeX must have corresponding dct:references in RDF",
      "Every dct:references must point to registered citation in citations.jsonld",
      "Every theorem/definition citing a source must have prov:Attribution",
      "Every section derived from external work should have prov:wasDerivedFrom",
      "All citation keys must match pattern: [lastname][year][shortname]"
    ]
  }
}
