@prefix catty: <https://metavacua.github.io/CategoricalReasoner/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dbr: <http://dbpedia.org/resource/> .
@prefix wd: <http://www.wikidata.org/entity/> .

# Intuitionistic Logic (LJ) - Heyting, 1930
catty:IntuitionisticLogic a catty:Logic, owl:NamedIndividual ;
    rdfs:label "Intuitionistic Logic (LJ)"@en ;
    rdfs:comment "Heyting's intuitionistic logic. Restricted succedent (single formula). Lacks Law of Excluded Middle (LEM) and Double Negation Elimination (DNE)."@en ;
    
    # External references
    owl:sameAs wd:Q176786 ;  # Wikidata: Intuitionistic logic
    skos:exactMatch dbr:Intuitionistic_logic ;
    dcterms:source <https://ncatlab.org/nlab/show/intuitionistic+logic> ;
    dcterms:bibliographicCitation "Heyting, A. (1930). Die formalen Regeln der intuitionistischen Logik. Sitzungsberichte der Preussischen Akademie der Wissenschaften." ;
    
    # Structural rules (W=1, C=1, E=1)
    catty:hasStructuralRule catty:Weakening, catty:Contraction, catty:Exchange ;
    
    # Sequent form
    catty:hasSequentForm catty:RestrictedSuccedent ;
    
    # Logical axioms (lacks classical axioms)
    catty:lacksLogicalAxiom catty:LawOfExcludedMiddle, catty:DoubleNegationElimination ;
    
    # Logical connectives
    catty:hasLogicalConnective catty:IntuitionisticImplication, catty:Conjunction, catty:Disjunction, catty:Negation ;
    
    # Lattice coordinates (horizontal: restricted, vertical: full)
    catty:latticeCoordinate "(restricted, full)" ;
    
    # Categorical properties
    catty:isQuantumSafe "false"^^xsd:boolean .