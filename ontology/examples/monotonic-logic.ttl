@prefix catty: <https://owner.github.io/Catty/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dbr: <http://dbpedia.org/resource/> .
@prefix wd: <http://www.wikidata.org/entity/> .

# Monotonic Logic
catty:MonotonicLogic a catty:Logic, owl:NamedIndividual ;
    rdfs:label "Monotonic Logic"@en ;
    rdfs:comment "Both antecedent and succedent restricted: A ⊢ B. Formal intersection of LJ and LDJ. No negation or implication (structurally inexpressible). Only positive lattice operations."@en ;

    # External references (no direct DBpedia/Wikidata; related to lattice theory)
    skos:related dbr:Lattice_(order) ;
    skos:related wd:Q715439 ;  # Lattice (order theory)
    dcterms:source <https://ncatlab.org/nlab/show/partial+order> ;

    # Structural rules (varies by family; typically full structural for basic variant)
    catty:hasStructuralRule catty:Exchange,
                            catty:WeakeningLeft,
                            catty:WeakeningRight,
                            catty:ContractionLeft,
                            catty:ContractionRight ;

    # Sequent restrictions (both restricted)
    catty:hasSequentRestriction catty:RestrictedAntecedent,
                                 catty:RestrictedSuccedent ;

    # Logical axioms (neither LEM nor LNC provable)
    catty:notProvableAxiom catty:LEM, catty:LNC ;
    catty:provesAxiom catty:Explosion ;  # If weakening present

    # Logical connectives (only positive operations)
    catty:hasConnective catty:Conjunction,    # ∧ (meet)
                        catty:Disjunction,     # ∨ (join)
                        catty:Bottom,          # ⊥
                        catty:Top ;            # ⊤

    # Position in lattice
    catty:inFamily catty:FullStructuralFamily ;
    catty:horizontalPosition catty:BottomOfDiamond ;  # Monotonic is meet of LJ and LDJ
    catty:verticalPosition catty:FullStructuralLayer ;

    # Morphisms
    catty:extendedByLogic catty:IntuitionisticLogic, catty:DualIntuitionisticLogic ;

    # Physical realizability
    catty:isQuantumSafe false ;  # Due to weakening/contraction
    catty:allowsCloning true ;
    catty:allowsErasure true ;

    # Computational interpretation
    catty:computationalInterpretation "Monotone functions over partially ordered types; lattice operations only"@en ;
    catty:curryHowardCorrespondence "Order-theoretic types; monotone maps"@en .
