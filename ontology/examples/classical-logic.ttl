@prefix catty: <https://owner.github.io/Catty/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dbr: <http://dbpedia.org/resource/> .
@prefix wd: <http://www.wikidata.org/entity/> .

# Classical Logic (LK) - Gentzen, 1935
catty:ClassicalLogic a catty:Logic, owl:NamedIndividual ;
    rdfs:label "Classical Logic (LK)"@en ;
    rdfs:comment "Gentzen's sequent calculus for classical logic. Unrestricted sequents: Γ ⊢ Δ. All structural rules present."@en ;

    # External references
    owl:sameAs wd:Q217699 ;  # Wikidata: Classical logic
    skos:exactMatch dbr:Classical_logic ;
    dcterms:source <https://ncatlab.org/nlab/show/classical+logic> ;
    dcterms:bibliographicCitation "Gentzen, G. (1935). Untersuchungen über das logische Schließen." ;

    # Structural rules (all present)
    catty:hasStructuralRule catty:Exchange,
                            catty:WeakeningLeft,
                            catty:WeakeningRight,
                            catty:ContractionLeft,
                            catty:ContractionRight ;

    # Sequent restrictions (none)
    catty:hasSequentRestriction catty:UnrestrictedAntecedent,
                                 catty:UnrestrictedSuccedent ;

    # Logical axioms
    catty:provesAxiom catty:LEM,       # Law of Excluded Middle
                      catty:LNC,       # Law of Non-Contradiction
                      catty:Explosion, # Ex falso quodlibet
                      catty:DoubleNegationElimination ;

    # Logical connectives
    catty:hasConnective catty:Conjunction,    # ∧
                        catty:Disjunction,     # ∨
                        catty:Negation,        # ¬
                        catty:Implication,     # →
                        catty:Biconditional,   # ↔
                        catty:Bottom,          # ⊥
                        catty:Top ;            # ⊤

    # Position in lattice
    catty:inFamily catty:FullStructuralFamily ;
    catty:horizontalPosition catty:TopOfDiamond ;  # Classical is terminal
    catty:verticalPosition catty:FullStructuralLayer ;

    # Physical realizability
    catty:isQuantumSafe false ;
    catty:allowsCloning true ;   # Contraction present
    catty:allowsErasure true .   # Weakening present

# Structural rules instances
catty:Exchange a catty:StructuralRule ;
    rdfs:label "Exchange"@en ;
    rdfs:comment "Order of premises is irrelevant"@en ;
    skos:related dbr:Exchange_rule .

catty:WeakeningLeft a catty:StructuralRule ;
    rdfs:label "Weakening (Left)"@en ;
    rdfs:comment "Add hypotheses freely to antecedent"@en ;
    skos:related dbr:Weakening .

catty:WeakeningRight a catty:StructuralRule ;
    rdfs:label "Weakening (Right)"@en ;
    rdfs:comment "Add conclusions freely to succedent"@en .

catty:ContractionLeft a catty:StructuralRule ;
    rdfs:label "Contraction (Left)"@en ;
    rdfs:comment "Merge duplicate hypotheses in antecedent"@en ;
    skos:related dbr:Contraction_rule .

catty:ContractionRight a catty:StructuralRule ;
    rdfs:label "Contraction (Right)"@en ;
    rdfs:comment "Merge duplicate conclusions in succedent"@en .

# Sequent restrictions
catty:UnrestrictedAntecedent a catty:SequentRestriction ;
    rdfs:label "Unrestricted Antecedent"@en ;
    rdfs:comment "Antecedent can have any number of formulas (Γ)"@en .

catty:UnrestrictedSuccedent a catty:SequentRestriction ;
    rdfs:label "Unrestricted Succedent"@en ;
    rdfs:comment "Succedent can have any number of formulas (Δ)"@en .

# Logical axioms
catty:LEM a catty:LogicalAxiom ;
    rdfs:label "Law of Excluded Middle"@en ;
    rdfs:comment "⊢ P ∨ ¬P"@en ;
    owl:sameAs wd:Q207531 ;
    skos:exactMatch dbr:Law_of_excluded_middle .

catty:LNC a catty:LogicalAxiom ;
    rdfs:label "Law of Non-Contradiction"@en ;
    rdfs:comment "¬(P ∧ ¬P) ⊢"@en ;
    owl:sameAs wd:Q207545 ;
    skos:exactMatch dbr:Law_of_noncontradiction .

catty:Explosion a catty:LogicalAxiom ;
    rdfs:label "Explosion (Ex Falso Quodlibet)"@en ;
    rdfs:comment "⊥ ⊢ Q"@en ;
    owl:sameAs wd:Q752952 ;
    skos:exactMatch dbr:Principle_of_explosion .

catty:DoubleNegationElimination a catty:LogicalAxiom ;
    rdfs:label "Double Negation Elimination"@en ;
    rdfs:comment "¬¬P ⊢ P"@en .

# Logical family
catty:FullStructuralFamily a catty:LogicalFamily ;
    rdfs:label "Full Structural Family"@en ;
    rdfs:comment "Logics with all structural rules present: E, W, C"@en ;
    catty:hasStructuralConfiguration "E=1, W_L=1, W_R=1, C_L=1, C_R=1"^^xsd:string .
