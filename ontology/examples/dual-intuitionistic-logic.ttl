@prefix catty: <https://owner.github.io/Catty/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dbr: <http://dbpedia.org/resource/> .
@prefix wd: <http://www.wikidata.org/entity/> .

# Dual-Intuitionistic Logic (LDJ) - Urbas, 1996
catty:DualIntuitionisticLogic a catty:Logic, owl:NamedIndividual ;
    rdfs:label "Dual-Intuitionistic Logic (LDJ)"@en ;
    rdfs:comment "Urbas's sequent calculus for dual-intuitionistic logic. Restricted antecedent: A ⊢ Δ (single hypothesis). Symmetric dual to LJ."@en ;

    # External references
    skos:related dbr:Intuitionistic_logic, wd:Q179899 ;
    dcterms:source <https://ncatlab.org/nlab/show/co-Heyting+algebra> ;
    dcterms:bibliographicCitation "Urbas, I. (1996). Dual-Intuitionistic Logic. Notre Dame J. Formal Logic 37(3): 440-451." ;

    # Structural rules (all present on RHS; LHS restricted makes left-side rules moot)
    catty:hasStructuralRule catty:Exchange,
                            catty:WeakeningRight,
                            catty:ContractionRight ;

    # Sequent restrictions
    catty:hasSequentRestriction catty:RestrictedAntecedent,
                                 catty:UnrestrictedSuccedent ;

    # Logical axioms (LNC absent; LEM present)
    catty:provesAxiom catty:LEM ;
    catty:notProvableAxiom catty:LNC ;

    # Logical connectives (dual to intuitionistic)
    catty:hasConnective catty:Conjunction, catty:Disjunction,
                        catty:Subtraction,  # Dual to implication
                        catty:Bottom, catty:Top ;

    # Position in lattice
    catty:inFamily catty:FullStructuralFamily ;
    catty:horizontalPosition catty:RightOfDiamond ;
    catty:verticalPosition catty:FullStructuralLayer ;

    # Morphisms
    catty:extendsLogic catty:MonotonicLogic ;
    catty:extendedByLogic catty:ClassicalLogic ;

    # Physical realizability
    catty:isQuantumSafe false ;
    catty:allowsCloning true ;
    catty:allowsErasure true ;

    # Computational interpretation
    catty:computationalInterpretation "Co-constructive computation; refutations as co-patterns"@en ;
    catty:curryHowardCorrespondence "Co-data types; co-recursion"@en .

# Sequent restriction
catty:RestrictedAntecedent a catty:SequentRestriction ;
    rdfs:label "Restricted Antecedent"@en ;
    rdfs:comment "Antecedent has at most one formula (A) or is empty"@en .

# Additional connective
catty:Subtraction a catty:LogicalConnective ;
    rdfs:label "Subtraction (¬)"@en ;
    rdfs:comment "Dual to implication in dual-intuitionistic logic"@en .
