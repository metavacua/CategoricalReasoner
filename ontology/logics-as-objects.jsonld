    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "dct": "http://purl.org/dc/terms/",
    "prov": "http://www.w3.org/ns/prov#",
    "dbo": "http://dbpedia.org/ontology/",
    "wd": "http://www.wikidata.org/entity/",
    "hasLogicalSignature": { "@id": "catty:hasLogicalSignature", "@type": "@id" },
    "hasLogicalAxiom": { "@id": "catty:hasLogicalAxiom", "@type": "@id" },
    "hasStructuralRule": { "@id": "catty:hasStructuralRule", "@type": "@id" },
    "hasSequentForm": { "@id": "catty:hasSequentForm", "@type": "@id" },
    "owl:sameAs": { "@type": "@id" }
  },
  "@graph": [
    {
      "@id": "catty:StandardSignature",
      "@type": "catty:LogicalSignature",
      "rdfs:comment": "Standard signature with common logical connectives (∧, ∨, ⊤, ⊥, ¬, ⊢)"
    },
    {
      "@id": "catty:LM",
      "@type": "catty:Logic",
      "rdfs:label": "LM (Minimal Base)",
      "rdfs:comment": "Minimal common sublogic for LJ and LDJ. No negation, no explosion, no LEM, no LNC.",
      "hasLogicalSignature": "catty:StandardSignature"
    },
    {
      "@id": "catty:LJ",
      "@type": "catty:Logic",
      "rdfs:label": "LJ (Intuitionistic Logic)",
      "rdfs:comment": "Extends LM with LNC and Explosion. Single-conclusion sequents (RHS restriction).",
      "hasSequentForm": "catty:SingleConclusionSequent",
      "hasLogicalSignature": "catty:StandardSignature",
      "hasStructuralRule": [
        "catty:LHSWeakening", "catty:RHSWeakening",
        "catty:LHSContraction", "catty:RHSContraction",
        "catty:LHSExchange", "catty:RHSExchange"
      ],
      "hasLogicalAxiom": ["catty:Axiom_LNC", "catty:Axiom_Explosion"],
      "owl:sameAs": "http://www.wikidata.org/entity/Q176786"
      },
    {
      "@id": "catty:LDJ",
      "@type": "catty:Logic",
      "rdfs:label": "LDJ (Dual Intuitionistic Logic)",
      "rdfs:comment": "Extends LM with LEM. Single-antecedent sequents (LHS restriction).",
      "hasSequentForm": "catty:SingleAntecedentSequent",
      "hasLogicalSignature": "catty:StandardSignature",
      "hasStructuralRule": [
        "catty:LHSWeakening", "catty:RHSWeakening",
        "catty:LHSContraction", "catty:RHSContraction",
        "catty:LHSExchange", "catty:RHSExchange"
      ],
      "hasLogicalAxiom": "catty:Axiom_LEM"
    },
    {
      "@id": "catty:LK",
      "@type": "catty:Logic",
      "rdfs:label": "LK (Classical Logic)",
      "rdfs:comment": "The terminal logic. Validates LEM, LNC, Explosion, and DNE. Full structural rules.",
      "hasSequentForm": "catty:MultiConclusionSequent",
      "hasLogicalSignature": "catty:StandardSignature",
      "hasStructuralRule": [
        "catty:LHSWeakening", "catty:RHSWeakening",
        "catty:LHSContraction", "catty:RHSContraction",
        "catty:LHSExchange", "catty:RHSExchange"
      ],
      "hasLogicalAxiom": ["catty:Axiom_LEM", "catty:Axiom_LNC", "catty:Axiom_Explosion", "catty:Axiom_DNE"],
      "owl:sameAs": "http://www.wikidata.org/entity/Q236975"
      },
      {
      "@id": "catty:LL",
      "@type": "catty:Logic",
      "rdfs:label": "LL (Linear Logic)",
      "rdfs:comment": "Substructural logic without weakening or contraction. Resource-sensitive.",
      "hasSequentForm": "catty:MultiConclusionSequent",
      "hasLogicalSignature": "catty:StandardSignature",
      "hasStructuralRule": ["catty:LHSExchange", "catty:RHSExchange"],
      "owl:sameAs": "http://www.wikidata.org/entity/Q841728"
      },
    {
      "@id": "catty:ALL",
      "@type": "catty:Logic",
      "rdfs:label": "ALL (Affine Linear Logic)",
      "rdfs:comment": "Linear logic with weakening. Allows erasure but no cloning.",
      "hasLogicalSignature": "catty:StandardSignature",
      "hasStructuralRule": [
        "catty:LHSWeakening", "catty:RHSWeakening",
        "catty:LHSExchange", "catty:RHSExchange"
      ]
    },
    {
      "@id": "catty:RLL",
      "@type": "catty:Logic",
      "rdfs:label": "RLL (Relevant Linear Logic)",
      "rdfs:comment": "Linear logic with contraction. Allows cloning but no erasure.",
      "hasLogicalSignature": "catty:StandardSignature",
      "hasStructuralRule": [
        "catty:LHSContraction", "catty:RHSContraction",
        "catty:LHSExchange", "catty:RHSExchange"
      ]
    },
    {
      "@id": "catty:Axiom_LEM",
      "@type": "catty:LogicalAxiom",
      "rdfs:label": "Law of Excluded Middle",
      "rdfs:comment": "A ∨ ¬A",
      "owl:sameAs": "http://www.wikidata.org/entity/Q468422"
    },
    {
      "@id": "catty:Axiom_LNC",
      "@type": "catty:LogicalAxiom",
      "rdfs:label": "Law of Non-Contradiction",
      "rdfs:comment": "¬(A ∧ ¬A)",
      "owl:sameAs": "http://www.wikidata.org/entity/Q868437"
    },
    {
      "@id": "catty:Axiom_Explosion",
      "@type": "catty:LogicalAxiom",
      "rdfs:label": "Principle of Explosion",
      "rdfs:comment": "⊥ ⊢ A (Ex Falso Quodlibet)",
      "owl:sameAs": "http://www.wikidata.org/entity/Q60190"
    },
    {
      "@id": "catty:Axiom_DNE",
      "@type": "catty:LogicalAxiom",
      "rdfs:label": "Double Negation Elimination",
      "rdfs:comment": "¬¬A ⊢ A",
      "owl:sameAs": "http://www.wikidata.org/entity/Q5300067"
    }
  ]
