{
  "@context": {
    "@version": 1.1,
    "catty": "http://catty.org/ontology/",
    "owl": "http://www.w3.org/2002/07/owl#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "dct": "http://purl.org/dc/terms/",
    "prov": "http://www.w3.org/ns/prov#",
    "dbo": "http://dbpedia.org/ontology/",
    "wd": "http://www.wikidata.org/entity/"
  },
  "@graph": [
    {
      "@id": "catty:LM",
      "@type": "catty:Logic",
      "rdfs:label": "LM (Minimal Common Sublogic)",
      "rdfs:comment": "A common sublogic to LJ and LDJ containing conjunction, disjunction, and units, but lacking negation. It extends to either LJ or LDJ.",
      "catty:hasLogicalSignature": {
        "@id": "catty:LM_Signature",
        "@type": "catty:LogicalSignature",
        "rdfs:comment": "Signature containing ∧, ∨, ⊤, ⊥"
      }
    },
    {
      "@id": "catty:LJ",
      "@type": "catty:Logic",
      "rdfs:label": "LJ (Intuitionistic Logic)",
      "rdfs:comment": "Intuitionistic sequent calculus. Invalidates one direction of double negation. Extends LM and is extended by LK.",
      "catty:hasSequentForm": "catty:SingleConclusionSequent",
      "catty:hasStructuralRule": [
        "catty:LHSWeakening", "catty:RHSWeakening",
        "catty:LHSContraction", "catty:RHSContraction",
        "catty:LHSExchange", "catty:RHSExchange"
      ],
      "catty:hasLogicalAxiom": "catty:Axiom_LNC",
      "owl:sameAs": "wd:Q7680564"
    },
    {
      "@id": "catty:LDJ",
      "@type": "catty:Logic",
      "rdfs:label": "LDJ (Dual Intuitionistic Logic)",
      "rdfs:comment": "Dual intuitionistic logic. Invalidates the other direction of double negation compared to LJ. Extends LM and is extended by LK.",
      "catty:hasSequentForm": "catty:SingleAntecedentSequent",
      "catty:hasStructuralRule": [
        "catty:LHSWeakening", "catty:RHSWeakening",
        "catty:LHSContraction", "catty:RHSContraction",
        "catty:LHSExchange", "catty:RHSExchange"
      ],
      "catty:hasLogicalAxiom": "catty:Axiom_LEM"
    },
    {
      "@id": "catty:LK",
      "@type": "catty:Logic",
      "rdfs:label": "LK (Classical Logic)",
      "rdfs:comment": "The terminal logic in this category. Validates both LEM and LNC. Full structural rules and multi-conclusion sequents.",
      "catty:hasSequentForm": "catty:MultiConclusionSequent",
      "catty:hasStructuralRule": [
        "catty:LHSWeakening", "catty:RHSWeakening",
        "catty:LHSContraction", "catty:RHSContraction",
        "catty:LHSExchange", "catty:RHSExchange"
      ],
      "catty:hasLogicalAxiom": ["catty:Axiom_LEM", "catty:Axiom_LNC"],
      "owl:sameAs": "wd:Q3525872"
    },
    {
      "@id": "catty:LL",
      "@type": "catty:Logic",
      "rdfs:label": "LL (Linear Logic)",
      "rdfs:comment": "Girard's linear logic. Resource-sensitive (no weakening or contraction). Extending LL with weakening and contraction results in LK.",
      "catty:hasSequentForm": "catty:MultiConclusionSequent",
      "catty:hasStructuralRule": ["catty:LHSExchange", "catty:RHSExchange"],
      "owl:sameAs": "wd:Q847878"
    },
    {
      "@id": "catty:ALL",
      "@type": "catty:Logic",
      "rdfs:label": "ALL (Affine Linear Logic)",
      "rdfs:comment": "Linear logic with weakening but no contraction. Implements a 'no cloning' principle but allows erasure.",
      "catty:hasStructuralRule": [
        "catty:LHSWeakening", "catty:RHSWeakening",
        "catty:LHSExchange", "catty:RHSExchange"
      ]
    },
    {
      "@id": "catty:RLL",
      "@type": "catty:Logic",
      "rdfs:label": "RLL (Relevant Linear Logic)",
      "rdfs:comment": "Linear logic with contraction but no weakening. Implements a 'no erasure' principle but allows cloning.",
      "catty:hasStructuralRule": [
        "catty:LHSContraction", "catty:RHSContraction",
        "catty:LHSExchange", "catty:RHSExchange"
      ]
    },
    {
      "@id": "catty:Axiom_LEM",
      "@type": "catty:LogicalAxiom",
      "rdfs:label": "Law of Excluded Middle",
      "rdfs:comment": "A ∨ ¬A"
    },
    {
      "@id": "catty:Axiom_LNC",
      "@type": "catty:LogicalAxiom",
      "rdfs:label": "Law of Non-Contradiction",
      "rdfs:comment": "¬(A ∧ ¬A)"
    }
  ]
}
