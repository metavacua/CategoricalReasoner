# =============================================================================
# SOLVER ENTITY - FULLY STANDARDIZED OPERATIONAL SPEC
# =============================================================================

<https://github.com/metavacua/CategoricalReasoner#solver>
    a schema:MathSolver ;
    schema:description "Categorical foundations for morphisms between logics" ;
    schema:name "Catty Categorical Reasoner" ;
    
    # Standard I/O
    schema:input <https://github.com/metavacua/CategoricalReasoner#docs> ;
    schema:output <https://github.com/metavacua/CategoricalReasoner#src> ;
    
    # **OMG DOL Logic Module** = sequent calculus specification
    dol:logicModule [
        a dol:LogicModule ;
        dol:logic "sequent-calculus" ;
        dol:imports <https://www.omg.org/spec/DOL/1.0/PDF/> ;
        rdfs:comment "Gentzen-style sequent calculus for subclassical logics"
    ] ;
    
    # **PROV-O Activity** = diagram chase (42s timeout)
    prov:qualifiedAssociation [
        a prov:Activity ;
        prov:activityKind "diagram-chase" ;
        prov:startedAtTime "2026-02-28T18:40:00Z"^^xsd:dateTime ;
        prov:endedAtTime "2026-02-28T18:40:42Z"^^xsd:dateTime ;
        prov:used <https://github.com/metavacua/CategoricalReasoner#docs>
    ] ;
    
    # **ISO Common Logic decidability** = PTIME via finite normalization
    cl:hasProperty [
        rdf:type cl:DecidableLogic ;
        cl:proofDepthBound 42 ;
        cl:timeComplexity "polynomial" ;
        rdfs:label "LLL-style finite depth normalization"
    ] ;
    
    # **OpenJDK 25 JSR 269** = annotation processor pipeline
    jsr269:processes <https://github.com/metavacua/CategoricalReasoner#docs> ;
    prov:generated <https://github.com/metavacua/CategoricalReasoner#src> ;
    
    # Updated for OpenJDK 25 (Feb 2026)
    schema:operatingSystem "Java 25" ;
    schema:softwareRequirements <https://key-project.org/#software> ;
    schema:authorNotes "KeY-verified sequent calculus morphism enumerator" ;
    # Bidirectional solver-source link
    dct:isReferencedBy <https://github.com/metavacua/CategoricalReasoner#src> ;
    # Solver relates to dissertation (witness/proof relationship)
    dct:isReferencedBy <https://github.com/metavacua/CategoricalReasoner#dissertation> .

