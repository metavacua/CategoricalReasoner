# =============================================================================
# SOLVER ENTITY (MathSolver)
# =============================================================================

<https://github.com/metavacua/CategoricalReasoner#solver>
    a schema:MathSolver ;
    schema:description "Categorical foundations for morphisms between logics" ;
    schema:name "Catty Categorical Reasoner" ;
    # Input: RDF/OWL specifications from docs
    schema:input <https://github.com/metavacua/CategoricalReasoner#docs> ;
    # Output: Java records as formal proof objects
    schema:output <https://github.com/metavacua/CategoricalReasoner#src> ;
    # Software requirements for solver operation
    schema:softwareRequirements <https://jena.apache.org/#software> ;
    schema:softwareRequirements <https://github.com/Galigator/openllet#software> ;
    schema:operatingSystem "Java 17+" ;
    # --- OPERATIONAL SPECIFICATION: Decidable Sequent Calculus (DOL/PROV-O) ---
    # Proof system: DOL Logic Module for sequent calculus
    dol:hasModule [
        a dol:LogicModule ;
        dol:logicTheory "sequent-calculus" ;
        dol:imports <http://importer.minibench.mr/Logic/SequentCalculus>
    ] ;
    # Search strategy: PROV-O Activity for diagram chase
    prov:wasAssociatedWith [
        a prov:Activity ;
        prov:activityKind "diagram-chase" ;
        prov:endedAtTime "PT42S"^^xsd:duration
    ] ;
    # Decidability: DOL logic decidability guarantee
    logic:hasDecidability [
        a logic:FiniteDepthNormalization ;
        logic:proofDepthBound 42
    ] ;
    # JSR 269 code generation: Java annotation processor
    javamodel:annotationProcessor [
        a javacompiler:AnnotationProcessor ;
        javacompiler:processes <https://github.com/metavacua/CategoricalReasoner#docs> ;
        prov:generated <https://github.com/metavacua/CategoricalReasoner#src>
    ] ;
    # Validation via formal methods
    schema:authorNotes "Verified via KeY theorem prover; morphisms between logics formally specified" ;
    # Bidirectional solver-source link
    dct:isReferencedBy <https://github.com/metavacua/CategoricalReasoner#src> ;
    # Solver relates to dissertation (witness/proof relationship)
    dct:isReferencedBy <https://github.com/metavacua/CategoricalReasoner#dissertation> .

