  "@graph": [
    {
      "@id": "Morphism_LM_to_LJ",
      "@type": "Extension",
      "rdfs:label": "LM → LJ",
      "rdfs:comment": "Extension by adding negation, LNC, and Explosion, and restricting the RHS.",
      "domain": "LM",
      "codomain": "LJ"
    },
    {
      "@id": "Morphism_LM_to_LDJ",
      "@type": "Extension",
      "rdfs:label": "LM → LDJ",
      "rdfs:comment": "Extension by adding negation and LEM, and restricting the LHS.",
      "domain": "LM",
      "codomain": "LDJ"
    },
    {
      "@id": "Morphism_LJ_to_LK",
      "@type": "AddLEM",
      "rdfs:label": "LJ → LK (Add LEM)",
      "rdfs:comment": "Extension of intuitionistic logic to classical logic by adding the Law of Excluded Middle and removing RHS restriction.",
      "domain": "LJ",
      "codomain": "LK"
    },
    {
      "@id": "Morphism_LDJ_to_LK",
      "@type": "AddLNC",
      "rdfs:label": "LDJ → LK (Add LNC)",
      "rdfs:comment": "Extension of dual-intuitionistic logic to classical logic by adding the Law of Non-Contradiction and removing LHS restriction.",
      "domain": "LDJ",
      "codomain": "LK"
    },
    {
      "@id": "Morphism_LL_to_ALL",
      "@type": "AddStructuralRule",
      "rdfs:label": "LL → ALL (Add Weakening)",
      "rdfs:comment": "Extension by adding LHS and RHS Weakening. Allows erasure.",
      "domain": "LL",
      "codomain": "ALL"
    },
    {
      "@id": "Morphism_LL_to_RLL",
      "@type": "AddStructuralRule",
      "rdfs:label": "LL → RLL (Add Contraction)",
      "rdfs:comment": "Extension by adding LHS and RHS Contraction. Allows cloning.",
      "domain": "LL",
      "codomain": "RLL"
    },
    {
      "@id": "Morphism_ALL_to_LK",
      "@type": "AddStructuralRule",
      "rdfs:label": "ALL → LK (Add Contraction)",
      "rdfs:comment": "Extension by adding contraction and classical axioms (LEM, LNC, DNE).",
      "domain": "ALL",
      "codomain": "LK"
    },
    {
      "@id": "Morphism_RLL_to_LK",
      "@type": "AddStructuralRule",
      "rdfs:label": "RLL → LK (Add Weakening)",
      "rdfs:comment": "Extension by adding weakening and classical axioms (LEM, LNC, DNE).",
      "domain": "RLL",
      "codomain": "LK"
    },
    {
      "@id": "Morphism_LL_to_LK",
      "@type": "Extension",
      "rdfs:label": "LL → LK",
      "rdfs:comment": "Extension of linear logic to classical logic by adding both weakening and contraction, and classical axioms.",
      "domain": "LL",
      "codomain": "LK"
    },
    {
      "@id": "Interpretation_LK_to_LJ",
      "@type": "Interpretation",
      "rdfs:label": "LK → LJ (Double Negation)",
      "rdfs:comment": "Interpretation via the Gödel-Gentzen translation.",
      "domain": "LK",
      "codomain": "LJ"
    },
    {
      "@id": "Adjoint_LK_LJ",
      "@type": "AdjointFunctors",
      "rdfs:label": "Adjoint Pair: LK ↔ LJ",
      "rdfs:comment": "Reflective subcategory relationship.",
      "prov:wasDerivedFrom": "https://en.wikipedia.org/wiki/Double-negation_translation"
    }
  ]
