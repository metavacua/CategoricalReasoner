repo:project
    a doap:Project ;
    rdfs:label "Catty Project" ;
    rdfs:comment "Categorical Reasoner: algorithms and implementations for modeling formal logics as categorical structures" ;
    doap:name "Catty" ;
    doap:description "Develops algorithms and implementations for modeling formal logics as categorical structures. Provides computational tools for representing logics as objects in categories, with morphisms capturing sequent restrictions and structural rules." ;
    doap:homepage <https://github.com/metavacua/CategoricalReasoner> ;
    doap:maintainer <https://github.com/metavacua> ;
    doap:category "Formal Methods", "Category Theory", "Logic", "Semantic Web" ;
    doap:programming-language "Java" ;
    doap:implements <https://www.w3.org/TR/sparql11-query/> ; # SPARQL 1.1
    doap:implements <https://www.w3.org/TR/rdf-schema/> ; # RDF Schema
    doap:implements <https://www.iso.org/standard/58068.html> ; # DOL (ISO/IEC 17347)
    doap:repository <https://github.com/metavacua/CategoricalReasoner> ;
    doap:bug-database <https://github.com/metavacua/CategoricalReasoner/issues> ;
    schema:isPartOf <https://github.com/metavacua/CategoricalReasoner> .

